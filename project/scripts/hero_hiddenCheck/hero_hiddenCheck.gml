for(var h=0;h<array_height_2d(personController.heros);h++) {
	switch(h)
	{
		case hero.adelyn:
		case hero.thomas:
			if player.value >= 500 heros[h, hero_hidden] = false
		break
		case hero.krown:
		case hero.gaben:
			if player.value >= 3000 heros[h, hero_hidden] = false
		break
		case hero.aaronwright:
		case hero.chicocrypto:
		case hero.cz:
			if player.value >= 2000 heros[h, hero_hidden] = false
		break
	}
}